<script>

  fetch("/getMyAllOrders").then((res) => {
    res.json().then((result) => {
      console.log(result);
      let temp="";
      data = result.res;
      data.forEach(async (u, index) => {
        // console.log(u)
        let i = index + 1;
        temp += "<tr>";
        temp += "<td>" + i + "</td>";
        // temp +="<td>"+u.sr=0 ;for(let i=0;i<=100;i++){sr=i}+"</td>";
        temp += "<td>" + u.plantation + "</td>";
        temp += "<td>" + u.plantation_name + "</td>";
        temp += "<td>" + new Date(u.date) + "</td>";
        temp += "<td>" + u.status + "</td>";
        temp += "</tr>";
      });
      console.log(temp);
      document.getElementById("dat").innerHTML = temp;
    });
  });
</script>

  <div class="heading-row mb-20">
